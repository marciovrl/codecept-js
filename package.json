{
	"name": "codecept",
	"version": "1.0.0",
	"description": "This project is to demonstrate a simple example of using the CodeceptJS.",
	"main": "index.js",
	"scripts": {
		"codeceptjs": "codeceptjs run --steps",
		"codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
		"codeceptjs:reporter": "HEADLESS=true codeceptjs run --steps --reporter mochawesome",
		"codeceptjs:ui": "codecept-ui --app --reporter mochawesome",
		"codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
		"codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
		"codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/marciovrl/codecept-js.git"
	},
	"author": "Marcio Lemos",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/marciovrl/codecept-js/issues"
	},
	"homepage": "https://github.com/marciovrl/codecept-js#readme",
	"devDependencies": {
		"@codeceptjs/configure": "^0.6.2",
		"@codeceptjs/examples": "^1.2.1",
		"@codeceptjs/ui": "^0.4.6",
		"codeceptjs": "^3.0.7",
		"config": "^3.3.6",
		"dotenv": "^10.0.0",
		"mochawesome": "^6.2.2",
		"playwright": "^1.12.2",
		"prettier": "^2.3.1"
	}
}
